    "sass": "^1.22.7"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development BABEL_ENV=client parcel app/index.html",
    "dev:docker:server": "nodemon -e js --verbose --legacy-watch --ignore app --ignore dist --ignore server --exec 'NODE_ENV=development BABEL_ENV=server node __dist/server'",
    "dev:server": "nodemon --verbose -e js,jsx,html --ignore 'dist/**/*' --ignore '__dist/**/*' --ignore app/imported.js --exec 'npm run build:dev && npm run wp:copyfiles'",
    "server": "NODE_ENV=production node dist/server",
    "build:dev": "rimraf __dist && npm run create-bundles:dev",
    "build": "rimraf __dist && npm run create-bundles",
    "create-bundles": "concurrently \"npm run create-bundle:client\" \"npm run create-bundle:server\"",
    "create-bundle:client": "BABEL_ENV=client parcel build app/client.js -d __dist/client --public-url /dist/client",
    "create-bundle:server": "BABEL_ENV=server parcel build server/index.js -d __dist/server --public-url /dist --target=node",
    "create-bundles:dev": "concurrently \"npm run create-bundle:client:dev\" \"npm run create-bundle:server:dev\"",
    "create-bundle:client:dev": "BABEL_ENV=client NODE_ENV=development parcel build app/client.js --no-minify -d __dist/client --public-url /dist/client",
    "create-bundle:server:dev": "BABEL_ENV=server NODE_ENV=development parcel build server/index.js --no-minify -d __dist/server --public-url /dist --target=node",
    "wp:copyfiles": "cp -Rp __dist/* ../src/app/themes/main/dist/"
  },
