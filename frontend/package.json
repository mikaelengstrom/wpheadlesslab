  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "nodemon": "^1.19.1",
    "parcel-bundler": "^1.12.3",
    "react-hot-loader": "^4.12.6",
